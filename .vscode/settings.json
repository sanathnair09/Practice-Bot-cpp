{
  "java.configuration.updateBuildConfiguration": "disabled",
  "java.import.gradle.enabled": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "bin/": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true,
    "**/*~": true
  },
  "C_Cpp.default.configurationProvider": "vscode-wpilib",
  "files.associations": {
    "*.inc": "cpp",
    "ios": "cpp",
    "locale": "cpp",
    "ostream": "cpp",
    "ratio": "cpp",
    "algorithm": "cpp",
    "core": "cpp",
    "istream": "cpp",
    "memory": "cpp",
    "future": "cpp",
    "functional": "cpp"
  }
}
